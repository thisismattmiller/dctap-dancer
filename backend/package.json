{
  "name": "backend",
  "version": "1.0.0",
  "description": "DCTap Editor Backend API",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lock-workspace": "node --import tsx/esm src/cli/lock-workspace.ts 2>/dev/null || node dist/cli/lock-workspace.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.0",
    "multer": "^1.4.5-lts.1",
    "sql.js": "^1.11.0",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "supertest": "^7.1.4",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^4.0.15"
  }
}
